      "tests/"
    ]
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.0.0",
    "guzzlehttp/guzzle": "^6.1",
    "illuminate/support": "^5.2",
    "vlucas/phpdotenv": "^2.2"
  },
  "require-dev": {
    "symfony/var-dumper": "^3.0",
    "phpunit/phpunit": "^5.2",
    "fzaninotto/faker": "^1.5"
  }
}
