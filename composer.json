{
  "name": "duffleman/lunophp",
  "description": "PHP Library for interacting with luno.io",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "George Miller",
      "email": "luno@duffleman.co.uk"
    }
  ],
  "autoload": {
    "psr-4": {
      "Duffleman\\Luno\\": "src/"
    }
  },
  "autoload-dev": {
